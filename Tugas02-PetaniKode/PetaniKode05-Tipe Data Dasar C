#include <stdio.h>

void main(){
    int age = 19;
    float weight = 55.1;
    double height = 177.31;
    char gender = 'M';
    printf("My name is Satria, I am %i years old and I am %.2f cm tall\n", age, height);
    printf("I am very light, weighing at only %.2f kg, and my gender is %c", weight, gender);
}
