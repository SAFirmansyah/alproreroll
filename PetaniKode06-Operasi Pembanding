#include <stdio.h>

void main(){
    int a, b;
    printf("Input Nilai A = "); scanf("%d", &a);
    printf("Input Nilai B = "); scanf("%d", &b);

    printf("a = %d\n", a);
    printf("b = %d\n", b);

    printf("a > b = %d\n", a > b);
    printf("a < b = %d\n", a < b);
    printf("a >= b = %d\n", a >= b);
    printf("a <= b = %d\n", a <= b);
    printf("a == b = %d\n", a == b);
    printf("a != b = %d\n", a != b);
}
