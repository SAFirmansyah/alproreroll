#include <stdio.h>

void main(){
    int a, b;
    printf("Input Nilai A = "); scanf("%d", &a);
    printf("Input Nilai B = "); scanf("%d", &b);

    printf("a = %d\n", a);
    printf("b = %d\n", b);

    // logika AND
    printf("a && b = %d\n", a && b);

    // logika OR
    printf("a || b = %d\n", a || b);

    // logika NOT
    printf("!a = %d\n", !a);
}
